<?xml version="1.0" encoding="UTF-8"?>

<logger name="Servibanca-log" class="org.jpos.q2.qbean.LoggerAdaptor">
  <log-listener class="org.jpos.util.SimpleLogListener" />

  <log-listener class="org.jpos.util.ProtectedLogListener">
    <property name="protect" value="" />
    <property name="wipe"    value="" />
  </log-listener>

 


  <log-listener class="org.jpos.util.RotateLogListener">
    <property name="file" value="log/Servibanca.log" />
    <property name="window" value="86400" />
    <property name="copies" value="90" />
    <property name="maxsize" value="100000000" />
  </log-listener>
</logger>

